<link href="css/blog.css" rel="stylesheet">

<section id="blog" class="content-section text-center">
    <div class="container">
        <div class="col-lg-8 col-lg-offset-2">
            <h2>Blog</h2>
            <p>Coming soon: my thoughts on everything</p>
            
            <% for (var i = 0; i < posts.length; i++) {%>
              <p class='tiny-text'><em><%= posts[i].title %></em></p>
              <p class='tiny-text'><%= posts[i].body %></p>
            <% } %>
        </div>
    </div>
</section>



